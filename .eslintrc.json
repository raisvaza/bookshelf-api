{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "eol-last": [
            "error",
            "never"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
    }
}
